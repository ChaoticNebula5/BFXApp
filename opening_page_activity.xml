<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/constraint_layout_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/opening_page_activity_constraint_layout_constraint_layout_background_color">

	<ImageView
		android:id="@+id/iphone_wallpapers_hd_image_view"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginEnd="@dimen/opening_page_activity_iphone_wallpapers_hd_image_view_margin_end"
		android:layout_marginStart="@dimen/opening_page_activity_iphone_wallpapers_hd_image_view_margin_start"
		android:layout_marginTop="@dimen/opening_page_activity_iphone_wallpapers_hd_image_view_margin_top"
		android:scaleType="centerCrop"
		android:src="@drawable/iphone_wallpapers_hd___high_quality_iphone_backgrounds"
		app:layout_constraintHorizontal_weight="1.22"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="-42dp"
		tools:layout_editor_absoluteY="-10dp"/>

	<View
		android:id="@+id/rectangle1_constraint_layout"
		android:layout_width="@dimen/opening_page_activity_rectangle1_constraint_layout_width"
		android:layout_height="@dimen/opening_page_activity_rectangle1_constraint_layout_height"
		android:alpha="0"
		android:background="@drawable/opening_page_activity_rectangle1_constraint_layout_background"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="81dp"
		tools:layout_editor_absoluteY="403dp"/>

	<Button
		android:id="@+id/view_button"
		android:layout_width="@dimen/opening_page_activity_view_button_width"
		android:layout_height="@dimen/opening_page_activity_view_button_height"
		android:layout_marginEnd="@dimen/opening_page_activity_view_button_margin_end"
		android:layout_marginTop="@dimen/opening_page_activity_view_button_margin_top"
		android:background="?attr/selectableItemBackground"
		android:fontFamily="@font/font_firacode_medium"
		android:gravity="center"
		android:text="@string/opening_page_activity_view_button_text"
		android:textColor="#FFFCFC"
		android:textSize="@dimen/opening_page_activity_view_button_text_size"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="95dp"
		tools:layout_editor_absoluteY="383dp"/>
</android.support.constraint.ConstraintLayout>
